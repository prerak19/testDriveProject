nday',\n      'Tuesday',\n      'Wednesday',\n      'Thursday',\n      'Friday',\n      'Saturday',\n    ];\n\n    let dateString = ` ${days[dateObj.getDay()]},  ${dateObj.toLocaleString(\n      'default',\n      {\n        month: 'long',\n      }\n    )} ${dateObj.getDate()}    `;\n    setSelectedDate(dateString);\n    setSelectedTime(\n      dateObj.toLocaleString('en-US', {\n        hour: 'numeric',\n        minute: 'numeric',\n        hour12: true,\n      })\n    );\n  }, [props.isActive]);\n\n  return (\n    <div style={{ display: `${props.currentPage === 7 ? 'block' : 'none'}` }}>\n      <div className=\"container-fluid bggray d-flex justify-content-center height100vh overflow-auto\">\n        <div className=\"bgwhite centerDiv1\">\n          {disCal === true ? \n          <div className=\"relativepos\">\n            <button className=\"close-button\" onClick={() => setDisplayCalendly(false)}>\n              <span className=\"icon\">&times;</span>\n            </button>\n            <InlineWidget\n              url={calendlyUrl}\n              pageSettings={{\n                backgroundColor: 'ffffff',\n                hideEventTypeDetails: true,\n                hideLandingPageDetails: true,\n              }}\n              prefill={calendlyPrefill}\n              pageSettings\n              utm={utmPrefill}\n              styles={{\n                height: '800px',\n              }}\n            />\n          </div> : <div>\n            <h1 className=\"confirm-heading\">Confirm your Test Drive</h1>\n            <div className=\"row\">\n              <div className=\"col-sm-12 col-md-6 col-lg-6 displaynone\">\n                <div className=\"marTopBot20\">\n                  <iframe\n                    className=\"confirmMap\"\n                    src=\"https://www.google.com/maps/embed?pb=!1m14!1m8!1m3!1d48320.39666380446!2d-73.965441!3d40.805449!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x89c2f63c75af2d65%3A0xf45542f6cb090cc9!2s2880%20Broadway%2C%20New%20York%2C%